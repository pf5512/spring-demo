<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
  <bean id="duke" class="edu.buet.cse.springidol.impl.BeanJuggler">
    <property name="beanCount" value="5"/>
  </bean>
  
  <bean id="poem" class="edu.buet.cse.springidol.impl.ToHelen"/>
  
  <bean id="alanPoe" class="edu.buet.cse.springidol.impl.PoeticJuggler">
    <constructor-arg name="poem" ref="poem"/>
    <constructor-arg name="beanCount" value="5"/>
  </bean>
  
  <bean id="stage" class="edu.buet.cse.springidol.impl.Stage" factory-method="getInstance"/>
  
  <bean id="ticket" class="edu.buet.cse.springidol.impl.Ticket" scope="prototype"/>
</beans>